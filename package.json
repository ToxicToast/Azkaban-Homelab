{
  "name": "azkaban",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "start:twitch-bot": "nx serve azkaban-twitch-bot",
    "start:twitch-events": "nx serve azkaban-twitch-events",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build --base=origin/main",
    "affected:e2e": "nx affected:e2e --base=origin/main",
    "affected:test": "nx affected:test --base=origin/main",
    "affected:test:coverage": "nx affected:test --codeCoverage",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "release": "npx semantic-release",
    "changelog": "auto-changelog --output README.md --template keepachangelog",
    "outdated": "npm outdated",
    "update:all": "npx npm-check-updates -u",
    "build:all": "nx run-many --target=build --all",
    "test:all": "nx run-many --all --target=test",
    "test:all:coverage": "nx run-many --all --target=test -- --codeCoverage",
    "test:all:watch": "nx run-many --all --target=test -- --watchAll",
    "e2e:all": "nx run-many --target=e2e --all",
    "lint:all": "nx run-many --target=lint --all",
    "prepare": "husky install",
    "prettier": "prettier --list-different"
  },
  "private": true,
  "dependencies": {
    "@mikro-orm/core": "^5.6.3",
    "@mikro-orm/mariadb": "^5.6.3",
    "@mikro-orm/mongodb": "^5.6.3",
    "@mikro-orm/mysql": "^5.6.3",
    "@mikro-orm/nestjs": "^5.1.5",
    "@mikro-orm/postgresql": "^5.6.3",
    "@mikro-orm/sqlite": "^5.6.3",
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.4.1",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/testing": "^9.2.1",
    "@nrwl/cli": "15.4.2",
    "@nrwl/eslint-plugin-nx": "15.4.2",
    "@nrwl/jest": "15.4.2",
    "@nrwl/linter": "15.4.2",
    "@nrwl/nest": "15.4.2",
    "@nrwl/node": "15.4.2",
    "@nrwl/nx-cloud": "15.0.2",
    "@nrwl/workspace": "15.4.2",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@twurple/api": "^5.3.1",
    "@twurple/auth": "^5.3.1",
    "@twurple/chat": "^5.3.1",
    "@twurple/eventsub-http": "^5.3.1",
    "@twurple/eventsub-ngrok": "^5.3.1",
    "@twurple/eventsub-ws": "^5.3.1",
    "@twurple/pubsub": "^5.3.1",
    "@types/jest": "29.2.5",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "auto-changelog": "^2.4.0",
    "eslint": "~8.31.0",
    "eslint-config-prettier": "8.6.0",
    "husky": "^8.0.2",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "nx": "15.4.2",
    "prettier": "^2.8.1",
    "semantic-release": "^19.0.5",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "~4.9.4"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "prepare": [
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/npm",
        "npmPublish": false
      },
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "yarn.lock",
          "README.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  },
  "collectCoverage": true,
  "coverageThreshold": {
    "global": {
      "branches": 50,
      "functions": 75,
      "lines": 75,
      "statements": 75
    }
  },
  "coverageDirectory": "../coverage"
}

